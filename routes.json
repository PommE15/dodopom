[{
  "settings": [ "master" ],
    "_frame_view": {
      "verbs": ["GET"],
      "path": "/",
      "call": "frame.index"
    },
    "_frame_list": {
      "verbs": ["GET"],
      "path": "/list",
      "call": "frame.list"
    },
    "_frame_updateForm": {
      "verbs": ["GET"],
      "path": "/updateForm",
      "call": "frame.updateForm"
    },
    "_frame_updateGuest": {
      "verbs": ["POST"],
      "path": "/updateForm",
      "call": "frame.updateFormPost"
    }
}]
